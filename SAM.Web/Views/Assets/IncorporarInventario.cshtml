                <div class="col-md-3">
                    @Html.EditorFor(model => model.ValueUpdateModel, new { maxlength = 11, htmlAttributes = new { @class = "form-control sam-moeda" } })
                    @Html.ValidationMessageFor(model => model.ValueUpdateModel, "", new { @class = "text-danger" })
                </div>*@
        </div>

