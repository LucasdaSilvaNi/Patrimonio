                <div class="col-md-4">
                    @Html.EditorFor(model => model.MaterialGroupDescription, new { htmlAttributes = new { @class = "form-control materialGroupDescription", @readonly = "readonly" } })
                </div>
            </div>
            <div class="form-group">
