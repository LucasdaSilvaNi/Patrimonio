@model SAM.Web.Models.Institution

@{ViewBag.Title = "Detalhes do Órgão ";}

<h2>@ViewBag.Title @Model.Description</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.Code)</dt>
        <dd>@Html.DisplayFor(model => model.Code)</dd>

        <dt>@Html.DisplayNameFor(model => model.Description)</dt>
        <dd>@Html.DisplayFor(model => model.Description)</dd>

        <dt>@Html.DisplayNameFor(model => model.NameManagerReduced)</dt>
        <dd>@Html.DisplayFor(model => model.NameManagerReduced)</dd>

        <dt>@Html.DisplayNameFor(model => model.ManagerCode)</dt>
        <dd>@Html.DisplayFor(model => model.ManagerCode)</dd>

        <dt>@Html.DisplayNameFor(model => model.flagImplantado)</dt>
        <dd>@Html.DisplayFor(model => model.flagImplantado)</dd>

        <dt>@Html.DisplayName("Integração SIAFEM")</dt>
        <dd>@Html.DisplayFor(model => model.flagIntegracaoSiafem)</dd>

        <dt></dt>
        <dd>
            <div class="form-group">
                @Html.Partial("_EditButtons")
            </div>
        </dd>

    </dl>
</div>

@section Scripts {
    <script>
        sam.transaction.controleTransacaoDetailsPadrao('@HttpContext.Current.User.Identity.Name', '@HttpContext.Current.Request.CurrentExecutionFilePath');
    </script>
}