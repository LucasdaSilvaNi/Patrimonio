@model SAM.Web.Models.RelationshipAuxiliaryAccountItemGroup

@{
    ViewBag.Title = "Detalhes do Relacionamento Grupo Material - Conta Contábil";
}

<h2>@ViewBag.Title</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayName("Grupo material")</dt>
        <dd>@Html.DisplayFor(model => model.RelatedMaterialGroup.Description)</dd>

        <br />
        <dt>@Html.DisplayName("Conta Contábil")</dt>
        <dd>@Html.DisplayFor(model => model.RelatedAuxiliaryAccount.Description)</dd>

        <dt></dt>
        <dd>
            <div class="form-group">
                @Html.Partial("_EditButtons")
            </div>
        </dd>

    </dl>
</div>

@section Scripts {
    <script>
        sam.transaction.controleTransacaoDetailsPadrao('@HttpContext.Current.User.Identity.Name', '@HttpContext.Current.Request.CurrentExecutionFilePath');
    </script>
}
