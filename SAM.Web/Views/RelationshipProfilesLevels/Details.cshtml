@model SAM.Web.Models.RelPerfilNivel

@{ViewBag.Title = "Detalhes do Relacionamento Perfis por Nível";}

<h2>@ViewBag.Title</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.IdPerfil)</dt>
        <dd>@Html.DisplayFor(model => model.PerfilRelacionado.Descricao)</dd>

        <dt>@Html.DisplayNameFor(model => model.IdNivel)</dt>
        <dd>@Html.DisplayFor(model => model.NivelRelacionado.Descricao)</dd>

        <dt></dt>
        <dd>
            <div class="form-group">
                @Html.Partial("_EditButtons")
            </div>
        </dd>
    </dl>
</div>

@section Scripts {
    <script>
        sam.transaction.controleTransacaoDetailsPadrao('@HttpContext.Current.User.Identity.Name', '@HttpContext.Current.Request.CurrentExecutionFilePath');
    </script>
}