@model SAM.Web.Models.MovementType

@{
    ViewBag.Title = "Detalhes do Tipo de Movimento";
}

<h2>@ViewBag.Title</h2>

<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>@Html.DisplayNameFor(model => model.Code)</dt>
        <dd>@Html.DisplayFor(model => model.Code)</dd>

        <dt>@Html.DisplayNameFor(model => model.Description)</dt>
        <dd>@Html.DisplayFor(model => model.Description)</dd>

        <dt>@Html.DisplayName("Grupo de Movimento")</dt>
        <dd>@Html.DisplayFor(model => model.RelatedGroupMoviment.Description)</dd>

        <dt>@Html.LabelFor(model => model.Status, "Ativo")</dt>
        <dd>@Html.DisplayFor(model => model.Status)</dd>

        <dt></dt>
        <dd>
            <div class="form-group">
                @Html.Partial("_EditButtons")
            </div>
        </dd>

    </dl>
</div>

@section Scripts {
    <script>
        sam.transaction.controleTransacaoDetailsPadrao('@HttpContext.Current.User.Identity.Name', '@HttpContext.Current.Request.CurrentExecutionFilePath');
    </script>
}
