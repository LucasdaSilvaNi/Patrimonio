    @{
        if (Model.ListagemBPsVinculados.Count() > 0)
        {
          <div class="form-group col-sm-12">
              <div class="form-control divGrid" style="height:360px; overflow: auto;">
                  <table class="table" id="result-tables-patrimonio">
                      <tr>
                          <th style="text-align:center; color:#337ab7">Sigla</th>
                          <th style="text-align:center; color:#337ab7">Chapa</th>
                          <th style="text-align:center; color:#337ab7">Grupo Material</th>
                          <th style="text-align:center; color:#337ab7">Item Material</th>
                          <th style="text-align:center; color:#337ab7">Valor Aquisição</th>
                          <th style="text-align:center; color:#337ab7">Valor Atual</th>
                          <th style="text-align:center; color:#337ab7">Valor Depreciação Acumulada</th>
                          <th style="text-align:center; color:#337ab7">Detalhes do BP</th>
                      </tr>
                      @foreach (var bpVinculado in Model.ListagemBPsVinculados)
                      {
                        <tr>
                            <td style="text-align:center"><span id="Sigla">@bpVinculado.Sigla</span></td>
                            <td style="text-align:center"><span id="Chapa">@bpVinculado.Chapa</span></td>
                            <td style="text-align:center"><span id="Chapa">@bpVinculado.GrupoMaterial</span></td>
                            <td style="text-align:center"><span id="Chapa">@bpVinculado.ItemMaterial</span></td>
                            <td style="text-align:center"><span id="Chapa">@bpVinculado.ValorAquisicao</span></td>
                            <td style="text-align:center"><span id="Chapa">@bpVinculado.ValorAtual</span></td>
                            <td style="text-align:center"><span id="Chapa">@bpVinculado.ValorDepreciacaoAcumulada</span></td>
                            <td style="text-align:center">
                                <span id="Detalhes BP">
                                    <a class="btnDetalhePendencia action-detalhePendencia" title="Detalhes do BP" style="cursor: pointer;" href="@Url.Action("Edit", "Assets", new { id = bpVinculado.Codigo })">
                                        <i class="glyphicon glyphicon-search detalheclass" />
                                        <span class="sr-only" aria-hidden="true">Detalhes do BP</span>
                                    </a>
                                </span>
                            </td>
                        </tr>
                      }
                  </table>
              </div>
          </div>
        }
